{
  "request": {
    "reportId": "ml_test_automation",
    "config": {
      "streamQuery": true,
      "reportConfig": {
        "id": "ml_test_automation",
        "mergeConfig": {
          "postContainer": "$container",
          "container": "$container",
          "basePath": "/mount/data/analytics/tmp",
          "reportPath": "ml_test_automation.csv",
          "rollup": 0
        },
        "labels": {
          "block_name": "Block name",
          "SUM(unique_submissions)": "total_content_plays_on_portal",
          "legend": "No of observations in started status currently",
          "total_content_plays_on_portal": "No of observations in started status currently",
          "organisation_name": "Organisation",
          "parent_channel": "parent_channel",
          "district_name": "District Name",
          "program_name": "Program Name",
          "state_name": "State Name",
          "solution_name": "Observation Name"
        },
        "dateRange": {
          "staticInterval": "LastDay",
          "granularity": "all",
          "intervalSlider": 0
        },
        "metrics": [
          {
            "metric": "total_content_plays_on_portal",
            "label": "total_content_plays_on_portal",
            "druidQuery": {
              "dataSource": "ml-obs-status",
              "limitSpec": {
                "type": "default",
                "limit": 50000,
                "columns": [
                  {
                    "dimension": "SUM(unique_submissions)",
                    "direction": "descending"
                  }
                ]
              },
              "filters": [
                {
                  "type": "equals",
                  "dimension": "status",
                  "value": "started"
                },
                {
                  "type": "equals",
                  "dimension": "private_program",
                  "value": "false"
                }
              ],
              "granularity": "all",
              "dimensions": [
                {
                  "fieldName": "program_name",
                  "aliasName": "Program Name"
                },
                {
                  "fieldName": "state_name",
                  "aliasName": "State Name"
                },
                {
                  "fieldName": "parent_channel",
                  "aliasName": "parent_channel"
                },
                {
                  "fieldName": "district_name",
                  "aliasName": "District Name"
                },
                {
                  "fieldName": "block_name",
                  "aliasName": "Block name"
                },
                {
                  "fieldName": "organisation_name",
                  "aliasName": "Organisation"
                },
                {
                  "fieldName": "solution_name",
                  "aliasName": "Observation Name"
                }
              ],
              "aggregations": [
                {
                  "fieldName": "unique_submissions",
                  "type": "longSum",
                  "name": "total_content_plays_on_portal"
                }
              ],
              "queryType": "groupBy"
            }
          }
        ],
        "output": [
          {
            "type": "csv",
            "metrics": [
              "total_content_plays_on_portal"
            ],
            "dims": [
              "date"
            ],
            "fileParameters": [
              "id",
              "dims"
            ]
          }
        ],
        "queryType": "groupBy"
      },
      "container": "$container",
      "store": "$store",
      "key": "$key"
    },
    "createdBy": "$createdBy",
    "description": "Big Number will give an overall count on number of observations in started status as of today",
    "reportSchedule": "DAILY"
  }
}