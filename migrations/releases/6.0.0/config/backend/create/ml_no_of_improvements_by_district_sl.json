                    "postContainer": "$postContainer",
                    "container": "$container",
                    "basePath": "/mount/data/analytics/tmp",
                    "reportPath": "ml_no_of_improvements_by_district_sl.csv",
                    "rollup": 0
                },
                "labels": {
                    "total_content_plays_on_app": "completed",
                    "total_content_plays_on_desktop": "inProgress",
                    "legend": "Improvement projects status by district",
                    "SUM(no_of_imp_with_evidence)": "total_content_plays",
                    "total_content_plays_on_portal": "started",
                    "district_name": "District Name",
                    "program_name": "Program Name",
                    "state_name": "State Name",
                    "total_content_plays": "evidence"
                },
                "dateRange": {
                    "staticInterval": "LastDay",
                    "granularity": "all",
                    "intervalSlider": 0
                },
                "metrics": [
                    {
                        "metric": "total_content_plays_on_portal",
                        "label": "total_content_plays_on_portal",
                        "druidQuery": {
                            "dataSource": "ml-project-status",
                            "limitSpec": {
                                "type": "default",
                                "limit": 50000,
                                "columns": [
                                    {
                                        "dimension": "SUM(unique_users)",
                                        "direction": "descending"
                                    }
                                ]
                            },
                            "filters": [
                                {
                                    "dimension": "state_name",
                                    "type": "isnotnull"
                                },
                                {
                                    "dimension": "district_name",
                                    "type": "isnotnull"
                                },
                                {
                                    "type": "equals",
                                    "dimension": "status_of_project",
                                    "value": "started"
                                },
                                {
                                    "type": "equals",
                                    "dimension": "private_program",
                                    "value": "false"
                                },
                                {
                                    "type": "regex",
                                    "dimension": "program_name",
                                    "value": "^((?!(?i)(test)).)*$"
                                }
                            ],
                            "granularity": "all",
                            "dimensions": [
                                {
                                    "fieldName": "state_name",
                                    "aliasName": "State Name"
                                },
                                {
                                    "fieldName": "program_name",
                                    "aliasName": "Program Name"
                                },
                                {
                                    "fieldName": "district_name",
                                    "aliasName": "District Name"
                                }
                            ],
                            "aggregations": [
                                {
                                    "fieldName": "unique_users",
                                    "type": "longSum",
                                    "name": "total_content_plays_on_portal"
                                }
                            ],
                            "queryType": "groupBy"
                        }
                    },
